___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setCookie = require('setCookie');
const getCookieValues = require('getCookieValues');
const makeString = require('makeString');
const makeNumber = require('makeNumber');
const getType = require('getType');
const Object = require('Object');

// Flags
const updateLifeSpan = data.ddUpdateLifeSpanUponEachExecution === true;
const skipIfEmpty = data.ddNotSetCookieOnUndefined === true;

// Duration handling
const durationUnit = data.ddCookieDuration;
const lifespanValue = makeNumber(data.ddCookieLifeSpan);
let multiplier = 1;
if (durationUnit === 'minutesDuration') multiplier = 60;
else if (durationUnit === 'hourDuration') multiplier = 3600;
else if (durationUnit === 'dayDuration') multiplier = 86400;
const maxAge = lifespanValue * multiplier;

// Domain and path
const domain = (data.setCookieDomain && data.inputCookieDomain) ? makeString(data.inputCookieDomain) : 'auto';
const path = (data.setCookiePath && data.inputCookiePath) ? makeString(data.inputCookiePath) : '/';

// Process table rows
const cookieRows = data.ddCookieNameandValueTable || [];
for (let i = 0; i < cookieRows.length; i++) {
  const row = cookieRows[i];
  const rawName = row.ddCookieName;
  const rawValue = row.ddCookieValue;

  const cookieName = makeString(rawName);
  const inputValue = makeString(rawValue);
  const valueType = getType(rawValue);
  const isValueExplicitlyValid = valueType !== 'undefined' && valueType !== 'null' &&
                                 inputValue !== '' && inputValue !== 'undefined' && inputValue !== 'null';

  const existingValues = getCookieValues(cookieName);
  const existingValue = (existingValues && existingValues.length > 0) ? makeString(existingValues[0]) : null;

  let finalValue = isValueExplicitlyValid ? inputValue : existingValue;

  if (skipIfEmpty && !isValueExplicitlyValid) {
    continue;
  }

  // If not skipping invalid values and still no value, store empty string
  if (!finalValue && !skipIfEmpty) {
    finalValue = '';
  }

  const isSameValue = existingValue === finalValue;
  const shouldUpdate = updateLifeSpan || !isSameValue || !existingValue;

  if (shouldUpdate) {
    const options = {
      path: path,
      domain: domain,
      'max-age': maxAge
    };
    setCookie(cookieName, finalValue, options);
  }
}

data.gtmOnSuccess();


